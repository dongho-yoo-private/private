// -------------------------
int GetGaussDistributionTable(float rad, sGdTable* table)
{
	if (IsInteger(rad)!=0)
	{
		return xGetGaussDistributionTable((UINT)rad, table);
	}

	sGdTable tbl1;
	sGdTable tbl2;

	memset(&tbl1, 0, sizeof(tbl1));
	if (xGetGaussDistributionTable((UINT)rad, &tbl1)!=0)
		return -1;

	if (xGetGaussDistributionTable((UINT)rad+1, &tbl2)!=0)
		return -1;

	float remain = rad-(float)((UINT)rad);
	UINT irad = (UINT)rad+1;


	float sum=0.0;
	for (int i=0; i<(int)irad+1; i++)
	{
		for (int j=0; j<(int)irad+1; j++)
		{
			table->table[i][j] = (tbl1.table[i][j]*(1.0-remain) + tbl2.table[i][j]*remain);

			if (i==0 && j==0)
			{
				sum+=table->table[i][j];
			}
			else if (i==0 || j==0)
			{
				sum+=table->table[i][j];
				sum+=table->table[i][j];
			}
			else
			{
				sum+=table->table[i][j];
				sum+=table->table[i][j];
				sum+=table->table[i][j];
				sum+=table->table[i][j];
			}
		}
	}


	for (int i=0; i<(int)irad+1; i++)
	{
		for (int j=0; j<(int)irad+1; j++)
		{
			table->table[i][j]=table->table[i][j]/sum;
		}
	}

	table->rad=irad;

	return 0;

}
