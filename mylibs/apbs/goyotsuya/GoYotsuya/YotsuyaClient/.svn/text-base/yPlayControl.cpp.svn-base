{

}

cyPlayControl::~cyPlayControl()
{
}

void cyPlayControl::DoDataExchange(CDataExchange* pDX)
{
	CDialog::DoDataExchange(pDX);
	DDX_Control(pDX, IDC_BTN_CALCULATE_END, m_btnCalculateEnd);
	DDX_Control(pDX, IDC_BTN_PLAY_CANCEL2, m_btnCancelPlay);
	DDX_Control(pDX, IDC_BTN_PLAY_ABORT, m_btnAbortPlay);
	DDX_Control(pDX, IDC_BTN_PLAY_WAIT, m_btnWaitPlay);
	DDX_Control(pDX, IDC_BTN_PLAY_END, m_btnReqQuit);
	DDX_Control(pDX, IDC_BTN_PASS, m_btnPass);
	DDX_Control(pDX, IDC_BTN_GIVEUP, m_btnLost);
}


BEGIN_MESSAGE_MAP(cyPlayControl, CDialog)
	ON_WM_PAINT()
	ON_BN_CLICKED(IDC_BTN_GIVEUP, &cyPlayControl::OnBnClickedBtnGiveup)
	ON_BN_CLICKED(IDC_BTN_PASS, &cyPlayControl::OnBnClickedBtnPass)
	ON_BN_CLICKED(IDC_BTN_PLAY_END, &cyPlayControl::OnBnClickedBtnPlayEnd)
	ON_BN_CLICKED(IDC_BTN_PLAY_WAIT, &cyPlayControl::OnBnClickedBtnPlayWait)
	ON_BN_CLICKED(IDC_BTN_PLAY_ABORT, &cyPlayControl::OnBnClickedBtnPlayAbort)
	ON_BN_CLICKED(IDC_BTN_PLAY_CANCEL2, &cyPlayControl::OnBnClickedBtnPlayCancel2)
	ON_BN_CLICKED(IDC_BTN_CALCULATE_END, &cyPlayControl::OnBnClickedBtnCalculateEnd)
END_MESSAGE_MAP()


